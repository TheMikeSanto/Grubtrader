<table class="mws-datatable-fn mws-table">
    <thead>
        <tr>
            <th>Organization</th>
            <th>First name</th>
            <th>Last name</th>
            <th>Email</th>
            <th>Phone</th>
            <th>Role</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
      <% @users.each do |user| %>
        <tr class="gradeX">
            <td><%= user.organization.name %></td>
            <td><%= user.fname %></td>
            <td><%= user.lname %></td>
            <td><%= user.email %></td>
            <td><%= user.phone %></td>
            <td><%= user.role.name %></td>             
            <%= render partial: "layouts/action_button", locals: {object: user} %>
        </tr>
      <% end %>
    </tbody>
</table>